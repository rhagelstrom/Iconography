            <genericcontrol name="Iconography" insertafter="name">
                <anchored position="insidetopleft" offset="30" height="20">
                    <right parent="rightanchor" anchor="left" offset="-5" relation="relative"/>
                </anchored>
                <icon>blank_Iconography</icon>
			</genericcontrol>
			<masterindexitem_name name="name">
